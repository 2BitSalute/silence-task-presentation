{
    "name": "silence-task-presentation",
    "displayName": "silence-task-presentation",
    "description": "Sets the presetation's close behavior to true and reveal behavior to never",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.96.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/npm/cli.git"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "silence-task-presentation.doIt",
                "title": "Silence task presentation in tasks.json"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src",
        "test": "vscode-test"
    },
    "devDependencies": {
        "@types/mocha": "^10.0.10",
        "@types/node": "20.x",
        "@types/vscode": "^1.96.0",
        "@typescript-eslint/eslint-plugin": "^8.17.0",
        "@typescript-eslint/parser": "^8.17.0",
        "@vscode/test-cli": "^0.0.10",
        "@vscode/test-electron": "^2.4.1",
        "eslint": "^9.16.0",
        "typescript": "^5.7.2"
    },
    "dependencies": {
        "json5": "^2.2.3"
    }
}